<script>
	$(document).ready(function () {

		/* ========== SIDEBAR TOGGLE (MOBILE + DESKTOP) ========== */
		$("#mobile_btn, #toggle_btn").on("click", function (e) {
			e.preventDefault();

			if (window.innerWidth <= 768) {
				$("body").toggleClass("sidebar-open");
			} else {
				$("body").toggleClass("mini-sidebar");
			}
		});

		/* ========== OVERLAY CLICK (MOBILE CLOSE) ========== */
		$("#sidebarOverlay").on("click", function () {
			$("body").removeClass("sidebar-open");
		});

		/* ========== FIRST LEVEL MENU (MASTERS) ========== */
		$("#sidebar-menu > ul > li.submenu > a").on("click", function (e) {
			e.preventDefault();

			let parent = $(this).parent();

			$("#sidebar-menu > ul > li.submenu").not(parent)
				.removeClass("active")
				.children("ul")
				.slideUp(250);

			parent.toggleClass("active");
			parent.children("ul").stop(true, true).slideToggle(250);
		});

		/* ========== SECOND LEVEL MENU ========== */
		$("#sidebar-menu .submenu-two > a").on("click", function (e) {
			e.preventDefault();

			let parent = $(this).parent();

			parent.siblings(".submenu-two")
				.removeClass("active")
				.children("ul")
				.slideUp(200);

			parent.toggleClass("active");
			parent.children("ul").stop(true, true).slideToggle(200);
		});

	});
</script>

