	<style>
		/* ===== NEW SIDEBAR UI – BLUE CLEAN ===== */

		.sidebar-menu {
			padding: 8px 10px;
		}

			.sidebar-menu ul {
				list-style: none;
				padding: 0;
				margin: 0;
			}

			/* Main link */
			.sidebar-menu li > a {
				display: flex;
				align-items: center;
				gap: 12px;
				padding: 11px 14px;
				font-size: 14px;
				color: #1f2937;
				border-radius: 6px;
				transition: all 0.25s ease;
				position: relative;
			}

				/* Icon */
				.sidebar-menu li > a i {
					font-size: 18px;
					color: #2563eb;
					min-width: 22px;
				}

				/* Hover */
				.sidebar-menu li > a:hover {
					background: #f1f5ff;
					color: #2563eb;
				}

			/* Active / open */
			.submenu.open > a,
			.sidebar-menu li.active > a {
				background: #e0edff;
				color: #2563eb;
				font-weight: 600;
			}

				/* Left indicator bar */
				.submenu.open > a::before,
				.sidebar-menu li.active > a::before {
					content: "";
					position: absolute;
					left: 0;
					top: 8px;
					bottom: 8px;
					width: 4px;
					background: #2563eb;
					border-radius: 0 4px 4px 0;
				}

		/* Arrow */
		.menu-arrow {
			margin-left: auto;
			font-size: 13px;
			color: #6b7280;
			transition: transform 0.25s ease;
		}

		.submenu.open > a .menu-arrow {
			transform: rotate(90deg);
		}

		/* Submenu */
		.sidebar-menu .submenu > ul {
			display: none;
			padding-left: 34px;
			margin-top: 6px;
			border-left: 1px dashed #dbeafe;
		}

		/* Submenu items */
		.sidebar-menu .submenu ul li a {
			padding: 8px 10px;
			font-size: 13.5px;
			color: #374151;
		}

			/* Submenu hover */
			.sidebar-menu .submenu ul li a:hover {
				color: #2563eb;
				background: transparent;
			}

		/* Menu title */
		.menu-title {
			font-size: 11px;
			font-weight: 600;
			color: #9ca3af;
			padding: 14px 10px 6px;
			text-transform: uppercase;
		}

	</style>
