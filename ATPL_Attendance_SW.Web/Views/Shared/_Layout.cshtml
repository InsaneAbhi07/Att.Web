	<script>
		$(document).on("click", ".submenu > a", function (e) {
			e.preventDefault();

			const parent = $(this).parent();

			$(".submenu").not(parent)
				.removeClass("open")
				.children("ul").slideUp(200);

			parent.toggleClass("open");
			parent.children("ul").stop(true, true).slideToggle(200);
		});
